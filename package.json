{
  "name": "express-server",
  "version": "1.0.0",
  "description": "base for dev",
  "main": "bin/www",
  "scripts": {
    "init": "node ./init.js",
    "server": "node ./bin/www",
    "test": "mocha -t 10000 --recursive tests/",
    "coverage": "nyc --reporter=lcov --reporter=text-summary _mocha --no-timeouts --recursive tests/ --reporter mochawesome",
    "open-report": "open ./mochawesome-report/mochawesome.html && open ./coverage/lcov-report/index.html",
    "apidoc": "apidoc -f .js -i ./routes/ -o ./apidoc/",
    "open-api": "open ./apidoc/index.html"
  },
  "dependencies": {
    "config": "3.3.6",
    "cookie-parser": "~1.4.5",
    "express": "~4.16.1",
    "log4js": "6.3.0",
    "simple-client": "^1.0.2"
  },
  "keywords": [
    "express",
    "compress",
    "middleware",
    "api",
    "gzip"
  ],
  "author": "KaiYang Zhang",
  "license": "MIT",
  "devDependencies": {
    "apidoc": "^0.27.1",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2",
    "nyc": "^15.1.0",
    "supertest": "^6.1.3"
  },
  "apidoc": {
    "title": "express-server",
    "name": "express-server apis",
    "description": "样例服务api",
    "url" : "/v1",
    "sampleUrl": "/v1"
  }
}
